<%= form_with(model: @photo, local: true) do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>

    <%= form.label :description %>
    <%= form.text_area :description %>
    
    <%= form.file_field :image %>
    <%= form.submit "Update Cat Photo" %>

    <%= link_to 'Back', photos_path %>
<% end %>